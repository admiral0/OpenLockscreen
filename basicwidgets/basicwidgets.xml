<package id="basicwidgets"
         name="Basic widgets"
         description="Set of basic widgets.">
    <widget name="Analogic clock"
            qmlfile="AnalogicClock.qml"
            hassettings="false">
        <size width="200" height="200" />
    </widget>
    <widget name="Picture frame"
            qmlfile="PictureFrame.qml"
            hassettings="true">
        <size width="200" height="200" />
    </widget>
    <widget name="Welcome"
            qmlfile="Welcome.qml"
            hassettings="false">
        <size width="400" height="100" />
    </widget>

    <!--<widget name="Minimalist analogic clock"
            qmlfile="MinimalistClock.qml"
            hassettings="false">
        <size width="200" height="200" />
    </widget>-->
    <dock name="Android dock"
          qmlfile="AndroidDock.qml">
        <size height="100" />
        <anchors top="false"
                 bottom="true"
                 right="true"
                 left="true" />
    </dock>
    <dock name="Maemo dock"
          qmlfile="MaemoDock.qml">
        <size width="480" height="80" />
        <anchors top="true"
                 bottom="false"
                 right="false"
                 left="true" />
    </dock>
</package>
